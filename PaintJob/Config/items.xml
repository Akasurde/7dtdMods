<configs>
	<append xpath="/items">	

		<item name="pjVehicle4x4TruckWhitePlaceable">
			<property name="Extends" value="vehicleMinibikePlaceable"/>
			<property name="Meshfile" value="#@modfolder:Resources/Vehicles.unity3d?jeep_white"/>
			<property name="EconomicValue" value="30000"/>
			<property class="Action1">
				<property name="Vehicle" value="pjVehicle4x4TruckWhite"/>
				<property name="VehicleSize" value="2.5, 2.1, 5"/>
			</property>
			<effect_group name="pjVehicle4x4TruckWhitePlaceable" tiered="false">
