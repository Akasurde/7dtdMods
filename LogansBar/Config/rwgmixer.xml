<logansbar>
	
	<!-- Add Logan's bar to the mixer -->
  	<append xpath="/rwgmixer/prefab_rules/prefab_rule[contains(@name,'downtown')]">
		<prefab name="../../Mods/LogansBar/Prefabs/logans_bar" prob="5" min_count="1" max_count="1" min_repeat_distance="500"/>
	</append>

  	<append xpath="/rwgmixer/prefab_rules/prefab_rule[contains(@name,'industrial')]">
		<prefab name="../../Mods/LogansBar/Prefabs/logans_bar" prob="5" min_count="1" max_count="1" min_repeat_distance="500"/>
	</append>

	<!-- Add Logan's bar to the mixer -->
  	<append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='bigcitytownPOIS']">
		<prefab name="../../Mods/LogansBar/Prefabs/logans_bar" prob="5" min_count="1" max_count="3" min_repeat_distance="500"/>
	</append>
  	<append xpath="/rwgmixer/prefab_rules/prefab_rule[@name='newtownPOIS']">
		<prefab name="../../Mods/LogansBar/Prefabs/logans_bar" prob="5" min_count="1" max_count="3" min_repeat_distance="500"/>
	</append>

</logansbar>