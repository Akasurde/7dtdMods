<configs>
	<append xpath="/items">	
		<item name="spFlagPole">
			<property name="HoldType" value="45"/>
			<property name="CustomIcon" value="flagPole" />		
			<property name="Meshfile" value="Items/Misc/sackPrefab"/>
			<property name="DropMeshfile" value="Items/Misc/sack_droppedPrefab"/>
			<property name="Material" value="Miron"/>
			<property name="Stacknumber" value="50"/> <!-- STK resource -->
			<property name="EconomicValue" value="12"/>
			<property name="Group" value="Resources"/>
			<property name="CraftingSkillGroup" value="craftSkillMiscellaneous"/>
			<property name="CreativeMode" value="Player"/>
		</item>
		
		<item name="spFlagCanvas"> 
			<property name="HoldType" value="45"/>
			<property name="CustomIcon" value="flagCanvas" />			
			<property name="Meshfile" value="Items/Misc/sackPrefab"/>
			<property name="DropMeshfile" value="Items/Misc/sack_droppedPrefab"/>
			<property name="Material" value="Mcloth"/>
			<property name="Stacknumber" value="25"/> <!-- STK resource -->
			<property name="RepairAmount" value="50"/>
			<property name="FuelValue" value="1"/>
			<property name="Weight" value="5"/>
			<property name="EconomicValue" value="5"/>
			<property name="Group" value="Resources"/>
			<property name="CraftingSkillGroup" value="craftSkillMiscellaneous"/>
		</item>

		<item name="spVehicle4x4TruckPlaceable">
			<property name="Extends" value="vehicleMinibikePlaceable"/>
			<property name="Meshfile" value="#@modfolder:Resources/Vehicles.unity3d?jeep_Prefab"/>
			<property name="EconomicValue" value="30000"/>
			<property class="Action1">
				<property name="Vehicle" value="spVehicle4x4Truck"/>
				<property name="VehicleSize" value="2.5, 2.1, 5"/>
			</property>
			<effect_group name="spVehicle4x4TruckPlaceable" tiered="false">
				<passive_effect name="ModSlots" operation="base_set" value="4"/>
				<passive_effect name="ModPowerBonus" operation="base_add" value="300" tags="EconomicValue"/>
				<passive_effect name="DegradationMax" operation="base_set" value="7500"/>
			</effect_group>
		</item>

	</append>
</configs>